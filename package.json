{
  "name": "pmboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest babel.config.js"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "transform": {
      "^.+\\.js$": [
        "babel-jest",
        {
          "configFile": "./babel.config.cjs"
        }
      ]
    },
    "coverageProvider": "v8",
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "statements": 85,
        "functions": 85,
        "lines": 85
      }
    }
  },
  "dependencies": {
    "body-parser": "^1.12.0",
    "bson": "6",
    "cookie-parser": "^1.3.4",
    "csurf": "^1.11.0",
    "debug": "4",
    "express": "^4.20.0",
    "express-session": "^1.10.4",
    "formidable": "^3.5.4",
    "fs": "^0.0.1-security",
    "http-proxy-middleware": "2.0.9",
    "jsdoc": "^4.0.3",
    "pg": "^8.12.0",
    "supertest": "^7.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "babel-jest": "^29.7.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
